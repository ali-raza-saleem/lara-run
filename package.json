{
  "name": "lara-run",
  "displayName": "Lara Run",
  "description": "Run Laravel code with color-coded results â€” all in your editor.",
  "version": "3.3.0",
  "publisher": "ali-raza-saleem",
  "icon": "resources/icons/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ali-raza-saleem/lara-run.git"
  },
  "bugs": {
    "url": "https://github.com/ali-raza-saleem/lara-run/issues"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages",
    "Debuggers"
  ],
  "keywords": [
    "laravel",
    "laravel playground",
    "artisan",
    "tinker",
    "tinkerwell",
    "code runner",
    "php runner",
    "tinker runner",
    "laravel runner",
    "lara runner",
    "laravel tinker",
    "run php",
    "run laravel",
    "repl",
    "interactive php",
    "code execution",
    "code",
    "runner",
    "playground"
  ],
  "activationEvents": [
    "onLanguage:php",
    "onCommand:myExtension.runPhpFile",
    "onCommand:myExtension.clearOutput",
    "onCommand:myExtension.focusSearchBar",
    "onCommand:myExtension.installPlayground"
  ],
  "main": "./dist/extension.min.js",
  "files": [
    "dist/",
    "assets",
    "resources/",
    "package.json",
    "docs/README.marketplace.md",
    "LICENSE"
  ],
  "contributes": {
    "commands": [
      {
        "command": "myExtension.runPhpFile",
        "title": "Run PHP File"
      },
      {
        "command": "myExtension.clearOutput",
        "title": "Clear Output"
      },
      {
        "command": "myExtension.focusSearchBar",
        "title": "Focus Search Bar"
      },
      {
        "command": "myExtension.installPlayground",
        "title": "Lara Run: Install"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+alt+r",
        "command": "myExtension.runPhpFile",
        "when": "editorLangId == php"
      },
      {
        "key": "ctrl+alt+c",
        "command": "myExtension.clearOutput",
        "when": "editorLangId == php || webviewFocus"
      },
      {
        "key": "ctrl+alt+f",
        "command": "myExtension.focusSearchBar",
        "when": "editorLangId == php || webviewFocus"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "myExtension.installPlayground",
          "when": "workspaceFolderCount > 0",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Lara Run",
      "properties": {
        "laraRun.playgroundFolder": {
          "type": "string",
          "default": ".playground",
          "description": "The folder inside the Laravel project where PHP files can be executed. Should be inside laravel project root directory."
        },
        "laraRun.appendOutput": {
          "type": "boolean",
          "default": true,
          "description": "If true appends new output to existing output, else replaces old output with new."
        }
      }
    }
  },
  "customConfig": {
    "tinkerScriptPath": "./resources/tinker.php"
  },
  "scripts": {
    "build": "webpack --mode production",
    "lint": "eslint src resources/js/ --ext .ts,.js --fix",
    "format": "prettier --write src resources/js/ resources/css/ ",
    "fix-style": "npm run format && npm run lint",
    "package": "npm run build && vsce package --out ./packages/",
    "package-patch": "npm version patch && npm run package",
    "package-minor": "npm version minor && npm run package",
    "package-major": "npm version major && npm run package"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@types/node": "^22.13.10",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vscode/test-electron": "^2.4.1",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "esbuild": "^0.25.1",
    "esbuild-loader": "^4.3.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "file-loader": "^6.2.0",
    "glob": "^11.0.1",
    "mark.js": "^8.11.1",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.5.3",
    "style-loader": "^4.0.0",
    "thread-loader": "^4.0.4",
    "ts-loader": "^9.5.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "alpinejs": "^3.14.8",
    "highlight.js": "^11.11.1"
  }
}